/**
 * 8-way Or gate: 
 * out = in[0] Or in[1] Or ... Or in[7]
 */
CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    Or(a=in[0], b=in[1], out=o0);
    Or(a=in[2], b=in[3], out=o1);
    Or(a=in[4], b=in[5], out=o2);
    Or(a=in[6], b=in[7], out=o3);

    Or(a=o0, b=o1, out=p);
    Or(a=o2, b=o3, out=q);
    Or(a=p, b=q, out=out);

}